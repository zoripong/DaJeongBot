<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dajeong.chatbot.dajeongbot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.SelectBotActivity" />
        <activity android:name=".Activity.InputInfoActivity" />
        <activity android:name=".Activity.CalendarActivity" />
        <activity android:name=".Activity.SettingActivity" />
        <activity android:name=".Activity.TutorialActivity" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.SigninActivity"></activity>
    </application>

</manifest>